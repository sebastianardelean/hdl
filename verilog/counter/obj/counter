    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.8, 8;
    %load/vec4 v0x5608345de100_0;
    %subi 1, 0, 8;
    %assign/vec4 v0x5608345de100_0, 0;
T_0.8 ;
